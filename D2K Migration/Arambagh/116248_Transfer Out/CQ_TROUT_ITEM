select rownum uk, 
       invcode,
       item_desc,         
       INVOICED_QUANTITY,
       code,
       category3,
       category1
from
(       
select a.invcode,
       b.code,
       b.category2 item_desc,
       b.category3,
       b.category1,
       sum(a.INVOICED_QUANTITY) INVOICED_QUANTITY
from ginview.LV_TROUT_ITEM a, GINVIEW.LV_ITEM b
where a.icode=b.code
group by a.invcode,
       b.category2,
         b.Category3,
         b.category1,
         b.code
         )item