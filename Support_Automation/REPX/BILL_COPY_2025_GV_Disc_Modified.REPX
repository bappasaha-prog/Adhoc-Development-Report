using System.Linq;
using System.Data;
using DevExpress.XtraReports.UI;

private void Report_BeforePrint(object sender, System.Drawing.Printing.PrintEventArgs e) {
    var ds = this.DataSource as DataSet;
    if (ds == null) return;

    if (ds.Tables.Contains("v_SalesPerson")) {
        var names = ds.Tables["v_SalesPerson"]
                      .AsEnumerable()
                      .Select(r =&gt; r.Field&lt;string&gt;("SalesPersonName"))
                      .Where(n =&gt; !string.IsNullOrEmpty(n))
                      .Distinct()
                      .ToArray();

        this.Parameters["ConcatSalesPersons"].Value = string.Join(", ", names);
    }
}</Script></Scripts>
